---
import { actions } from "astro:actions";
import Layout from "../../layouts/Layout.astro";

const { slug = '' } = Astro.params

const { data, error } = await Astro.callAction(actions.getEntityBySlug, {
  slug,
  contentType: 'blog'
})

if(error) return Astro.redirect('/404')

