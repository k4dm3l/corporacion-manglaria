{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/404/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/blog/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/blog/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/projects/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/projects/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}